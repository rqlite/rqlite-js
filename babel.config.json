{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "add-module-exports"
  ],
  "env": {
    "development": {
      "retainLines": true,
      "sourceMaps": "both"
    }
  }
}
